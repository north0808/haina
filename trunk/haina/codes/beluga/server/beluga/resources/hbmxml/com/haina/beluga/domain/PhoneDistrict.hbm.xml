<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.haina.beluga.domain.PhoneDistrict"
            table="PhoneDistrict"
    >

        <cache 
            usage="read-write"
        />

    <id
        name="id"
            column="ID"
            unsaved-value="null"
    >

    <generator
        class="uuid.hex"
    >

    </generator>

    </id>

        <version
            name="version"
            column="VERSION"
            type="java.lang.Long"
        />

        <property
            name="districtNumber"
                    column="districtNumber"
                length="20"
        >

        </property>

        <property
            name="range"
                    column="range"
                length="45000"
        >

        </property>

        <property
            name="feeType"
                    column="feeType"
                length="20"
        >

        </property>

        <property
            name="districtCity"
                    column="districtCity"
                length="20"
        >

        </property>

        <property
            name="districtProvince"
                    column="districtProvince"
                length="20"
        >

        </property>

        <property
            name="updateFlg"
                    column="updateFlg"
        >

        </property>

        <property
            name="pingyinCity"
                    column="pingyinCity"
                length="20"
        >

        </property>

        <property
            name="weatherCityCode"
                    column="weatherCityCode"
                length="20"
        >

        </property>

    </class>    

</hibernate-mapping>

